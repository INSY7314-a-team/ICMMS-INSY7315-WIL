// Test all endpoints sequentially
async function testAllEndpoints() {
    console.log('üöÄ testAllEndpoints function called');

    try {
        // Check if apiEndpoints exists
        if (typeof apiEndpoints === 'undefined') {
            console.error('‚ùå apiEndpoints object is not defined!');
            alert('‚ùå Error: API endpoints configuration not found. Please refresh the page.');
            return;
        }

        console.log('üìã apiEndpoints object:', apiEndpoints);
        console.log('üî¢ apiEndpoints keys:', Object.keys(apiEndpoints));

        // Get test button for use in finally block
        const testButton = document.querySelector('[onclick*="testAllEndpoints"]');
        console.log('üîò Test button found:', testButton);

        // Reset test results
        testResults = [];
        currentTestIndex = 0;

        // Collect all endpoints from all categories
        const allEndpoints = [];
        console.log('üìö Collecting endpoints...');

        Object.entries(apiEndpoints).forEach(([categoryName, categoryData]) => {
            console.log(`üìÇ Processing category: ${categoryName}`, categoryData);
            categoryData.endpoints.forEach((endpoint, endpointIndex) => {
                allEndpoints.push({
                    ...endpoint,
                    category: categoryName,
                    categoryName: categoryData.name,
                    endpointIndex: endpointIndex
                });
            });
        });

        totalTests = allEndpoints.length;
        console.log(`üéØ Found ${totalTests} endpoints to test:`, allEndpoints);

        if (totalTests === 0) {
            console.log('‚ùå No endpoints found to test!');
            alert('‚ùå No endpoints found to test!');
            return;
        }

        // Show progress section and hide default content
        console.log('üìä Showing progress section...');
        const progressSection = document.getElementById('test-progress-section');
        const defaultContent = document.getElementById('default-content');
        const resultsSection = document.getElementById('test-results-section');

        console.log('üîç DOM elements found:', { progressSection, defaultContent, resultsSection });

        // Check if elements exist before manipulating them
